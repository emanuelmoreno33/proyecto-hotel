@model IEnumerable<hotel.Models.ReservedFrom>

@{
    ViewBag.Title = "Index";
}

<h2>Opciones para reservar</h2>

<p>
    <button type="button" class="btn btn-primary" onclick="location.href='@Url.Action( "Create")'">Agregar</button>
</p>
<table class="table">
    <tr>
        <th>
            Opción de reservación:
        </th>
    </tr>

    @foreach (var option in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modeloption => option.reservationMethod)
            </td>
        </tr>
    }
</table>