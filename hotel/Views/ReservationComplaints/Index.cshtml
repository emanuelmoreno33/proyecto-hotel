@model IEnumerable<hotel.Models.ReservationComplaint>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            Comentario
        </th>
        <th>
           Fecha registrada
        </th>
        <th>
           Departamento
        </th>
        <th>
            No. de reservacion
        </th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.comment)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.dateComplaint)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Departament.type)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Reservation.reservationID)
        </td>
    </tr>
}

</table>
